{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ]
    }
}